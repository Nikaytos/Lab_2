<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.text.Font?>


<AnchorPane style="-fx-background-color: #1abc9c; -fx-border-color: #16a085; -fx-border-width: 6px;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.dlg.newMonsters.NewMonstersController">
   <children>
      <GridPane hgap="20.0" vgap="20.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="Перший монстр" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <Label text="Другий монстр" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.valignment="CENTER">
               <font>
                  <Font name="System Bold" size="18.0" />
               </font>
            </Label>
            <ComboBox fx:id="comboBox1" prefHeight="26.0" prefWidth="250.0" style="-fx-background-color: #dfe6e9; -fx-effect: dropshadow(one-pass-box, rgba(0,0,0,0.5), 10, 0, -3, 3);" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER" />
            <ComboBox fx:id="comboBox2" prefHeight="26.0" prefWidth="250.0" style="-fx-background-color: #dfe6e9; -fx-effect: dropshadow(one-pass-box, rgba(0,0,0,0.5), 10, 0, -3, 3);" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Button fx:id="button1" mnemonicParsing="false" style="-fx-background-color: #55efc4; -fx-text-fill: #222f3e; -fx-effect: dropshadow(one-pass-box, rgba(0,0,0,0.5), 10, 0, -3, 3);" text="OK" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
            <Button fx:id="button2" mnemonicParsing="false" style="-fx-background-color: #55efc4; -fx-text-fill: #222f3e; -fx-effect: dropshadow(one-pass-box, rgba(0,0,0,0.5), 10, 0, -3, 3);" text="Cancel" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER">
               <font>
                  <Font name="System Bold" size="14.0" />
               </font>
            </Button>
         </children>
         <padding>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </padding>
      </GridPane>
   </children>
</AnchorPane>
